{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "MONGO_URI": "@mongo_uri",
      "ACCESS_TOKEN_SECRET": "@access_token_secret"
    }
  }
  